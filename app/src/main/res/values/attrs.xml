<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TickTockView">
        <attr name="tickTextSize" format="dimension" />
        <attr name="tickRingThickness" format="dimension" />
        <attr name="tickDotRadius" format="dimension" />
        <attr name="tickTextColor" format="color" />
        <attr name="tickEmptyRingColor" format="color" />
        <attr name="tickFillRingColor" format="color" />
        <attr name="tickMiddleColor" format="color" />
        <attr name="tickText" format="string" />
        <attr name="tickMoveCounterClockwise" format="boolean" />
        <attr name="tickAutoFitText" format="boolean" />
        <attr name="tickCircleDuration" format="enum">
            <enum name="minute" value="0"/>
            <enum name="total_time" value="1" />
        </attr>
    </declare-styleable>
    <attr name="palette" format="reference"/>

    <attr name="highContrastReverseTextColor" format="reference"/>
    <attr name="mediumContrastReverseTextColor" format="reference"/>
    <attr name="lowContrastReverseTextColor" format="reference"/>

    <attr name="highContrastTextColor" format="reference"/>
    <attr name="mediumContrastTextColor" format="reference"/>
    <attr name="lowContrastTextColor" format="reference"/>
</resources>
